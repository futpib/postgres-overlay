{
  "name": "postgres-overlay",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT OR GPL-3.0+",
  "scripts": {
    "test": "xo && nyc ava",
    "test:e2e": "nyc ava test/e2e/index.js"
  },
  "bin": {
    "postgres-overlay": "./cli.js"
  },
  "homepage": "https://github.com/futpib/postgres-overlay",
  "ava": {
    "verbose": true,
    "files": [
      "**/*.test.js",
      "!test/e2e/**/*.js"
    ]
  },
  "devDependencies": {
    "ava": "^6.0.0",
    "coveralls": "^3.0.5",
    "eslint-config-xo-overrides": "^1.4.0",
    "nyc": "^14.1.1",
    "xo": "^0.24.0"
  },
  "xo": {
    "extends": [
      "eslint-config-xo-overrides"
    ],
    "rules": {
      "new-cap": "off"
    },
    "overrides": [
      {
        "files": "test/e2e/**/*.js",
        "rules": {
          "ava/no-ignored-test-files": "off"
        }
      }
    ]
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "make-promises-safe": "^5.0.0",
    "pg": "^7.11.0",
    "pg-escape": "^0.2.0",
    "sanctuary": "^2.0.0"
  }
}
